let a = 7, b = 8, c=9

let result3 = a<b || a<c

console.log(result3);

console.log("____________________________________________________________________________________________________");

let a1 = 7, b1 = 8, c1=4

let result4 = a1<b1 || a1<c1

console.log(result4);

console.log("____________________________________________________________________________________________________");

let a2 = 7, b2 = 8, c2=9

let result5 = a2>b2 || a2>c2

console.log(result5);

console.log("____________________________________________________________________________________________________");

let result6 = a2>b2 || a2>c2

let notResult = !result6

console.log(notResult);